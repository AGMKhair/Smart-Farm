<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.securesoftbd.smart_farm">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/project_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.securesoftbd.smart_farm.ViewProfileActivity"></activity>
        <activity android:name="com.securesoftbd.smart_farm.ProfileActivity" />
        <activity android:name="com.securesoftbd.smart_farm.AddProfileActivity" />
        <activity android:name="com.securesoftbd.smart_farm.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>